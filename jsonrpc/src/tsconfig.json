{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"declaration": true,
		"stripInternal": true,
		"lib": [ "es2015", "es2015.promise" ],
		"outDir": "../lib",
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}